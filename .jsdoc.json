{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["node_modules/@minininja/engine/src", "src", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(out)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "mini.ninja Engine + three.js",
    "disableSort": false
  },
  "opts": {
    "destination": "./out/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/jsdoc-template"
  }
}
